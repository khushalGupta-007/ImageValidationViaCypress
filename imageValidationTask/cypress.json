{
  "baseUrl": "https://www.google.com",
  "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*"],
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "threshold": 0.01
      }
    }
  },
  "video": false,
  "defaultCommandTimeout": 8500,
  "viewportWidth": 1920,
  "viewportHeight": 1080,
  "watchForFileChanges": false
}
